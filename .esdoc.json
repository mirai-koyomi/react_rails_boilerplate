{
  "source": "./client/src/js/",
  "destination": "./doc/js/",
  "includes": ["\\.(js|jsx)$"],
  "plugins": [
    {"name": "esdoc-standard-plugin", "option": {"enable": true}},
    {"name": "esdoc-external-ecmascript-plugin", "option": {"all": true}},
    {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}},
    {"name": "esdoc-flow-type-plugin", "option": {"enable": true}},
    {"name": "esdoc-jsx-plugin", "option": {"enable": true}},
    {"name": "esdoc-react-plugin", "option": {"enable": true}}
  ]
}
